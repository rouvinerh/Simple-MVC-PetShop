    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>206c8308-9be4-438a-b73e-6beb8a698dc1</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="7.0.16" />
